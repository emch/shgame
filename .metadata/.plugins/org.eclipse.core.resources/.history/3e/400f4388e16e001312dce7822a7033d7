#include <stdlib.h>

#include "shdata.h"

int main() {
	// Initialization
	SHLogger* myLogger = CreateLogger("shserver.log");

	// Main loop and threads management
	int i = 0;
	i = 45;
	flogf(myLogger,"%d",i);

	// Garbage collector
	DestroyLogger(myLogger);

	// End of program
	return EXIT_SUCCESS;
}

