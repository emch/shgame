SHCharacter* LoadCharacter(char* dataloc, char* name) {
	// Open file specified by dataloc
	FILE* datafile = fopen(dataloc,'r');

	// Create object in memory
	SHCharacter* res = malloc(sizeof(SHCharacter));

	// Close data file
	fclose(datafile);

	return res;
}

