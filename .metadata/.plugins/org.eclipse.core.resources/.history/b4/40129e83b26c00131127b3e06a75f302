#include <stdlib.h>
#include <stdio.h>
#include "SHCharacter.h"

/**
 *
 */
SHCharacter* LoadCharacter(char* dataloc, char* name) {
	// Open file specified by dataloc
	FILE* datafile = fopen(dataloc,'r');

	// Create object in memory
	SHCharacter* character = malloc(sizeof(SHCharacter));

	// Close data file
	fclose(datafile);

	// Result
	return character;
}

/*
 *
 */
void ClearCharacter(SHCharacter* character) {
	// Free space in memory
	free(character);
}

