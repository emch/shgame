#include <stdio.h>
#include <stdlib.h>
#include <time.h>

#include "shdata.h"

SHDice* InitDice(SH_DICE_TYPE type) {
	SHDice* res = malloc(sizeof(SHDice));

	res->type 	= type;
	res->value	= 1;

	return res;
}

void DestroyDice(SHDice* dice) {
	free(dice);
}
